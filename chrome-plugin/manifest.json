{
  "manifest_version": 3,
  "name": "SunoDownloader",
  "version": "0.9.2",
  "description": "Download all your published Suno songs.",
  "permissions": [
    "downloads",
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*.suno.com/*"
  ],
  "action": {
    "default_icon": "SunoDownloader.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
