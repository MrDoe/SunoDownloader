{
  "manifest_version": 3,
  "name": "SunoDownloader",
  "version": "0.6.2",
  "description": "Download all your published Suno songs.",  "license": "MIT",  "permissions": [
    "downloads",
    "scripting",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*.suno.com/*"
  ],
  "action": {
    "default_icon": "SunoDownloader.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "@suno-bulk-downloader",
      "strict_min_version": "142.0"
    }
  }
}
